def caesar_encrypt(text, k):
  result =""
  for ch in text:
    if ch.isupper():
      result += chr((ord(ch)-ord('A') + k) % 26 + ord('A'))
    elif ch.islower():
      result += chr((ord(ch)- ord('a') + k) % 26 + ord('a'))
    else:
      result += ch
  return result
message = input("enter the message:")
print("\ncaeser plain text output:")
for k in range(1,2):
  encrypted = caesar_encrypt(message, k)
  print(f"k = {k:2d} -> {encrypted}")
